        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nwx.job.quartz.mapper.SysUserMapper">

    <select id="findUserById" parameterType="java.lang.String" resultType="com.nwx.job.quartz.pojo.SysUser">
